{
  "extends": [
    "standard",
    "react-app/jest",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "plugin:storybook/recommended",
    "plugin:cypress/recommended",
    "prettier"
  ],
  "plugins": ["@stylistic/js"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "18.2"
    }
  },
  "rules": {
    "react/prop-types": "off",
    "eqeqeq": [2, "smart"],
    "no-console": "warn",
    "camelcase": "off",
    "no-eval": "error",
    "import/first": "error"
  }
}

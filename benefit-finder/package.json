{
  "name": "benefit-finder",
  "version": "0.1.3.beta.1",
  "private": true,
  "engines": {
    "node": "20.x.x"
  },
  "scripts": {
    "mv:uswds:usagov": "bash ../mv-uswds-usagov-theme.sh",
    "prestart": "npm run mv:uswds:usagov",
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest watch",
    "test:no-watch": "vitest run",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write '**/*.{js,jsx,json}'",
    "lint:scss": "stylelint 'src/**/*.scss'",
    "lint:scss:fix": "stylelint 'src/**/*.scss' --fix",
    "lint:js": "eslint . --ext .js,.jsx,.json",
    "lint:js:fix": "npm run lint:js -- --fix",
    "postinstall": "cd .. && husky install benefit-finder/.husky",
    "lint-staged": "lint-staged",
    "generate:component": "plop",
    "prebuild:storybook": "npm run mv:uswds:usagov && npm run build",
    "dev:storybook": "npm run prebuild:storybook && storybook dev -p 6006",
    "build:storybook": "npm run prebuild:storybook && storybook build",
    "cy:prebuild:storybook": "NODE_ENV=test npm run mv:uswds:usagov && npm run build",
    "cy:dev:storybook": "NODE_ENV=test npm run cy:prebuild:storybook && storybook dev -p 6006",
    "cy:dev:components": "NODE_ENV=test npm run cy:prebuild:storybook && cypress open",
    "cy:build:storybook": "NODE_ENV=test npm run cy:prebuild:storybook && storybook build --test",
    "cy:run:component:chrome": "NODE_ENV=test npx cypress run --component --browser chrome",
    "cy:run:e2e:chrome": "NODE_ENV=test npx cypress run --browser chrome",
    "cy:run": "NODE_ENV=test npm run cy:run:component:chrome && npm run cy:run:e2e:chrome",
    "cy:run:pipeline": "NODE_ENV=test concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\"   \"http-server ./storybook-static --port 6006 --silent\"   \"wait-on tcp:6006 && npm run cy:run\" -Y"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@storybook/addon-a11y": "^7.6.10",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-mdx-gfm": "^7.6.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/builder-vite": "^7.6.12",
    "@storybook/react-vite": "^7.6.12",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@stylistic/eslint-plugin-js": "^1.6.2",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@uswds/uswds": "^3.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-storybook": "^0.6.12",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "lint-staged": "^13.2.2",
    "plop": "^3.1.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.4",
    "prettier-eslint": "^16.3.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-scss": "^3.0.0",
    "sass": "^1.70.0",
    "storybook": "^7.6.17",
    "stylelint": "^15.10.1",
    "stylelint-config-standard-scss": "^9.0.0",
    "vite": "^5.0.12",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.2.2",
    "wait-on": "^7.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

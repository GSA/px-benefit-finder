{
  "name": "benefit-finder",
  "version": "0.1.3.beta.1",
  "private": true,
  "engines": {
    "node": "20.x.x"
  },
  "scripts": {
    "check:uswds:version": "sh ../check-uswds-version.sh",
    "mv:uswds:usagov": "sh ../mv-uswds-usagov-theme.sh",
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest watch",
    "test:no-watch": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:generate-output": "vitest --run --reporter=json --outputFile=.jest-test-results.json",
    "format": "prettier --write '**/*.{js,jsx}'",
    "lint:scss": "stylelint 'src/**/*.scss'",
    "lint:scss:fix": "stylelint 'src/**/*.scss' --fix",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "postinstall": "cd .. && husky install benefit-finder/.husky",
    "lint-staged": "lint-staged",
    "generate:component": "plop",
    "prebuild:storybook": "npm run build && npm run mv:uswds:usagov",
    "dev:storybook": "npm run prebuild:storybook && storybook dev -p 6006",
    "build:storybook": "npm run prebuild:storybook && storybook build",
    "cy:run:component:chrome": "NODE_ENV=test npx cypress run --component --browser chrome",
    "cy:run:e2e:chrome": "NODE_ENV=test npx cypress run --browser chrome"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@storybook/addon-a11y": "^7.6.10",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-mdx-gfm": "^7.6.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/react-vite": "^7.6.10",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@uswds/uswds": "^3.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.4",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "lint-staged": "^13.2.2",
    "plop": "^3.1.2",
    "prettier": "^3.2.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.63.4",
    "storybook": "^7.6.10",
    "stylelint": "^15.10.1",
    "stylelint-config-standard-scss": "^9.0.0",
    "vite": "^5.0.12",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.2.2",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

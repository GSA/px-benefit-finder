<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY ns_applicant "file:/**/Applicant#">
  <!ENTITY wsrr "http://**/xmlns/prod/serviceregistry/6/1/model#">
]>

<rdf:RDF
  xmlns:xsd="&xsd;"
  xmlns:rdf="&rdf;"
  xmlns:rdfs="&rdfs;"
  xmlns:owl="&owl;"
  xmlns:ns_applicant="&ns_applicant;"
  xmlns:wsrr="&wsrr;"
>

  <owl:Ontology rdf:about="&ns_applicant;ApplicantOntology">
    <owl:imports rdf:resource="http://www.ibm.com/xmlns/prod/serviceregistry/6/1/model"/>
    <wsrr:prefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string">applicant</wsrr:prefix>
    <rdfs:label>A applicant classification system.</rdfs:label>
    <rdfs:comment>Benefit applicant and some superclasses.</rdfs:comment>
  </owl:Ontology>

  <owl:Class rdf:about="&ns_applicant;Applicant">
    <rdfs:label>Applicant</rdfs:label>
    <rdfs:comment>Top-level root class for applicant.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="&ns_applicant;Deceased">
    <rdfs:subClassOf rdf:resource="&ns_applicant;Applicant"/>
    <rdfs:label>e</rdfs:label>
    <rdfs:comment>Middle-level applicant class.</rdfs:comment>
  </owl:Class>

  ...

</rdf:RDF>
